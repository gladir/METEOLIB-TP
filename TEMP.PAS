Function StrToUpper(S:String):String;
Var
 I:Byte;
Begin
 For I:=1 to Length(S)do Begin
  If S[I] in['a'..'z']Then S[I]:=Chr(Ord(S[I])-32);
 End;
 StrToUpper:=S;
End;

Function GetCountryTemperature(CountryName:String;Var Data:CountryGeoRecord):Boolean;
Var
 I:Integer;
Begin
 FillChar(Data,SizeOf(Data),0);
 For I:=Low(Country) to High(Country)do Begin
  If StrToUpper(StrPas(Country[I].Country))=StrToUpper(CountryName) Then Begin
   Data:=Country[I];
   Exit;
  End;
 End;
End;

Function GetCapitalTemperature(CapitalName:String;Var Data:CountryGeoRecord):Boolean;
Var
 I:Integer;
Begin
 FillChar(Data,SizeOf(Data),0);
 For I:=Low(Country) to High(Country)do Begin
  If StrToUpper(StrPas(Country[I].Capital))=StrToUpper(CapitalName) Then Begin
   Data:=Country[I];
   Exit;
  End;
 End;
End;

